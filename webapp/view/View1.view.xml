<mvc:View
        controllerName="SAP_SARAH_ML_Project.controller.View1"
        xmlns:html="http://www.w3.org/1999/xhtml"
        xmlns:mvc="sap.ui.core.mvc"
        displayBlock="true"
        xmlns:l="sap.ui.layout"
        xmlns:u="sap.ui.unified"
        xmlns:core="sap.ui.core"
        xmlns="sap.m">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<content>
					<Panel id="__panel0" headerText="Test App UI" expandable="true" expanded="true">
				    <content>
				        <HBox width="100%" id="__hbox0">
			                <VBox width="100%" direction="Column" id="__vbox0">
			                	<VBox class="grid-container" id="grid-container">
				                	<HBox class="grid-row">
				                		<VBox id="grid-cell-00" class="grid-cell"><sap.ui.core:Icon id="grid-cell-icon-00" xmlns:sap.ui.core="sap.ui.core" class="grid-icon" src="sap-icon://overlay" size="4em"/></VBox>
				                		<VBox id="grid-cell-01" class="grid-cell"><sap.ui.core:Icon id="grid-cell-icon-01" xmlns:sap.ui.core="sap.ui.core" class="grid-icon" src="sap-icon://overlay" size="4em"/></VBox>
				                		<VBox id="grid-cell-02" class="grid-cell"><sap.ui.core:Icon id="grid-cell-icon-02" xmlns:sap.ui.core="sap.ui.core" class="grid-icon" src="sap-icon://overlay" size="4em"/></VBox>
				                	</HBox>
				                	<HBox class="grid-row">
				                		<VBox id="grid-cell-10" class="grid-cell"><sap.ui.core:Icon id="grid-cell-icon-10" xmlns:sap.ui.core="sap.ui.core" class="grid-icon" src="sap-icon://overlay" size="4em"/></VBox>
				                		<VBox id="grid-cell-11" class="grid-cell"><sap.ui.core:Icon id="grid-cell-icon-11" xmlns:sap.ui.core="sap.ui.core" class="grid-icon" src="sap-icon://overlay" size="4em"/></VBox>
				                		<VBox id="grid-cell-12" class="grid-cell"><sap.ui.core:Icon id="grid-cell-icon-12" xmlns:sap.ui.core="sap.ui.core" class="grid-icon" src="sap-icon://overlay" size="4em"/></VBox>
				                	</HBox>
				                	<HBox class="grid-row">
				                		<VBox id="grid-cell-20" class="grid-cell"><sap.ui.core:Icon id="grid-cell-icon-20" xmlns:sap.ui.core="sap.ui.core" class="grid-icon" src="sap-icon://overlay" size="4em"/></VBox>
				                		<VBox id="grid-cell-21" class="grid-cell"><sap.ui.core:Icon id="grid-cell-icon-21" xmlns:sap.ui.core="sap.ui.core" class="grid-icon" src="sap-icon://overlay" size="4em"/></VBox>
				                		<VBox id="grid-cell-22" class="grid-cell"><sap.ui.core:Icon id="grid-cell-icon-22" xmlns:sap.ui.core="sap.ui.core" class="grid-icon" src="sap-icon://overlay" size="4em"/></VBox>
				                	</HBox>
			                	</VBox>
			                	<Button text="Randomize" width="100px" id="__button0" press="randomizeData"/>
			                </VBox>
			                <VBox width="100%" direction="Column" id="__vbox1">
			                	<core:HTML
                                id="fileHTML"
                                content='&lt;div&gt;&lt;input type=&quot;file&quot; id=&quot;previewImg&quot;&gt;&lt;br&gt;&lt;img id=&quot;image&quot;&gt;&lt;/div&gt;'
                                />
		                        <Button text="Get Zip" width="100px" id="__button1" press="zipImg"/>
			                </VBox>
				        </HBox>
				    </content>
				    </Panel>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>